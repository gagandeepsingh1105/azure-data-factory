{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "carsalespipeline-adf"
		},
		"ls_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls'"
		},
		"ls_azure_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_azure_databricks'"
		},
		"ls_azure_sql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sql'"
		},
		"ls_azure_sqldb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sqldb'"
		},
		"ls_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://carsalesdeltalake.dfs.core.windows.net/"
		},
		"ls_azure_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0225-165112-4i4zgfcn"
		},
		"ls_azure_sql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azure-demo-srvr.database.windows.net"
		},
		"ls_azure_sql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure-demo-db"
		},
		"ls_azure_sql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "demo-admin"
		},
		"ls_azure_sqldb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "carsales-dbserver.database.windows.net"
		},
		"ls_azure_sqldb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "carsales-db"
		},
		"ls_azure_sqldb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "carsalesadmin"
		},
		"ls_git_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "carsalespipeline-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_accountKey": {
						"value": "[parameters('ls_adls_accountKey')]"
					},
					"ls_azure_databricks_accessToken": {
						"value": "[parameters('ls_azure_databricks_accessToken')]"
					},
					"ls_azure_sql_password": {
						"value": "[parameters('ls_azure_sql_password')]"
					},
					"ls_azure_sqldb_password": {
						"value": "[parameters('ls_azure_sqldb_password')]"
					},
					"ls_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_properties_typeProperties_url')]"
					},
					"ls_azure_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_azure_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_azure_sql_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_server')]"
					},
					"ls_azure_sql_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_database')]"
					},
					"ls_azure_sql_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sql_properties_typeProperties_userName')]"
					},
					"ls_azure_sqldb_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sqldb_properties_typeProperties_server')]"
					},
					"ls_azure_sqldb_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sqldb_properties_typeProperties_database')]"
					},
					"ls_azure_sqldb_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sqldb_properties_typeProperties_userName')]"
					},
					"ls_git_properties_typeProperties_url": {
						"value": "[parameters('ls_git_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}